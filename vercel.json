{
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/classes",
      "destination": "/index.html"
    },
    {
      "source": "/classes/druid1",
      "destination": "/index.html"
    },
    {
      "source": "/classes/(.*)",
      "destination": "/index.html"
    },
    {
      "source": "/not-found",
      "destination": "/index.html"
    },
    {
      "source": "/:path*",
      "destination": "/index.html"
    }
  ]
}
